<?xml version="1.0" encoding="UTF-8" ?>
<site-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../dtds/site-conf.xsd">

	<request-map uri="stockCheckApproved" >
		<description>库存盘点审批通过后</description>
		<security auth="true"/>
		<event type="groovy" path="component://eiip/script/stock/check/stockCheckApproved.groovy"/>
		<response name="success" type="none" />
	</request-map>
	<request-map uri="stockTransferApproved" >
		<description>库存调拨审批通过后</description>
		<security auth="true"/>
		<event type="groovy" path="component://eiip/script/stock/transfer/stockTransferApproved.groovy"/>
		<response name="success" type="none" />
	</request-map>

</site-conf>
